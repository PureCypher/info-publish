# Discord Bot Configuration
# Copy this file to .env and fill in your actual values

# Your Discord bot token (get from https://discord.com/developers/applications)
DISCORD_TOKEN=your_discord_bot_token_here

# Privileged Intents Configuration
# Set to 'true' if you have enabled privileged intents in Discord Developer Portal
# Set to 'false' to run without privileged intents (limited functionality)
PRIVILEGED_INTENTS=false

# Optional: Set log level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO


# Docker and Runtime Configuration
# Memory optimization for Python in containers
MALLOC_TRIM_THRESHOLD_=131072

# Python runtime optimizations
PYTHONUNBUFFERED=1
PYTHONDONTWRITEBYTECODE=1
PYTHONIOENCODING=utf-8